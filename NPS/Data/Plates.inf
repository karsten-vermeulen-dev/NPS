FontNameDealerPostcode=Arial_bold_7mm
Dimension=520,111
MinAllowedGap=0
MaxAllowedSpace=1
MaxAllowedCharacters=7
IsCar=1
IsLegal=1
IsCarFont=1
IsSpaceRequired=1
IsSideBadgeAllowed=1
IsTwoLineRegistration=0

